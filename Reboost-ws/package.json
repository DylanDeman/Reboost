{
  "name": "reboost_service",
  "version": "1.0.0",
  "packageManager": "yarn@4.5.0",
  "prisma": {
    "schema": "src/data/schema.prisma",
    "seed": "tsx ./src/data/seed.ts"
  },
  "scripts": {
    "start:dev": "tsx watch --env-file .env --inspect=0.0.0.0:9001 src/index.ts",
    "build": "tsc",
    "lint": "eslint .",
    "migrate:dev": "prisma migrate dev",
    "migrate:test": "env-cmd -f .env.test prisma migrate dev --skip-seed",
    "test": "env-cmd -f .env.test jest --runInBand",
    "test:coverage": "env-cmd -f .env.test jest --runInBand --coverage"
  },
  "dependencies": {
    "@koa/cors": "^5.0.0",
    "@koa/router": "^13.1.0",
    "@prisma/client": "^5.21.1",
    "argon2": "^0.41.1",
    "config": "^3.3.12",
    "helmet": "^8.0.0",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "koa": "^2.15.3",
    "koa-bodyparser": "^4.4.1",
    "koa-helmet": "^8.0.1",
    "koa-router": "^13.0.1",
    "prisma": "^5.21.1",
    "winston": "^3.14.2"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.26.0",
    "@eslint/js": "^9.11.1",
    "@faker-js/faker": "^9.9.0",
    "@stylistic/eslint-plugin": "^2.8.0",
    "@types/config": "^3.3.5",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9",
    "@types/koa": "^2.15.0",
    "@types/koa-bodyparser": "^4.3.12",
    "@types/koa__cors": "^5",
    "@types/koa__router": "^12.0.4",
    "@types/node": "^22.7.4",
    "@types/supertest": "^6.0.2",
    "env-cmd": "^10.1.0",
    "eslint": "^9.11.1",
    "eslint-plugin-jest": "^28.9.0",
    "jest": "^29.7.0",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.1",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.8.0"
  },
  "engines": {
    "node": "20.6.0",
    "yarn": "4.4.0"
  }
}
